<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ChipTransport" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorAccent">@color/colorAcent</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorDarkPrimary</item>
        <item name="colorOnPrimary">@color/colorBlanco</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/colorNegro</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="btnRaisedLogin" parent="Theme.AppCompat.Light">
        <item name="colorButtonNormal">@color/colorAcent</item>
        <item name="colorControlHighlight">@color/colorBlanco</item>
        <item name="android:textColor">@color/colorBlanco</item>
    </style>

    <style name="Theme.ChipTransport.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ChipTransport.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ChipTransport.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>